@model DYV.Models.PlaceViewModels.UnsharePlacesViewModel

<h3>Are you sure you want to stop sharing?</h3>
<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>
    
    <form asp-action="Unshare">
        <div class="form-actions no-color">
            <input type="submit" value="Stop Sharing" class="btn btn-default" /> |
            <a asp-action="Index">Back to Places</a>
        </div>
    </form>
</div>
